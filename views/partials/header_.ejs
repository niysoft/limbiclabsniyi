<style>
    .col-md-4 {
        min-width: 400px;
    }

    .pick-numbers, .pick_numbers_ {
        float: left;
        margin-right: 6px;
        margin-left: 6px;
        text-align: center;
        height: 40px;
        width: 40px;
        border: 1px #cccccc solid;
        border-radius: 20px;
        font-size: 20px;
        line-height: 40px;
        margin-bottom: 16px;
        cursor: pointer;
    }

    .pick-numbers-color {
        margin-right: 6px;
        margin-left: 6px;
        text-align: center;
        height: 40px;
        width: 40px;
        border: 1px #8e24aa solid;
        color: #FFFFFF;
        border-radius: 20px;
        font-size: 20px;
        line-height: 40px;
        margin-bottom: 16px;
        cursor: pointer;
        background-color: #8e24aa !important;
    }

    .lds-hourglass {
        display: inline-block;
        position: relative;
        width: 64px;
        height: 64px;
    }

    .lds-hourglass:after {
        content: " ";
        display: block;
        border-radius: 50%;
        width: 0;
        height: 0;
        margin: 6px;
        box-sizing: border-box;
        border: 26px solid #cef;
        border-color: #cef transparent #cef transparent;
        animation: lds-hourglass 1.2s infinite;
    }

    @keyframes lds-hourglass {
        0% {
            transform: rotate(0);
            animation-timing-function: cubic-bezier(0.55, 0.055, 0.675, 0.19);
        }
        50% {
            transform: rotate(900deg);
            animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
        }
        100% {
            transform: rotate(1800deg);
        }
    }

    .notie-alert, .notie-container {
        z-index: 10000000;
    }
</style>
